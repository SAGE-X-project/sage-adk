{
  "dashboard": {
    "title": "SAGE ADK Agent Overview",
    "tags": ["sage-adk", "overview"],
    "timezone": "browser",
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(sage_adk_requests_total[5m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(sage_adk_requests_total{status=\"error\"}[5m])",
            "legendFormat": "Errors",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Response Time (P95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(sage_adk_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "go_memstats_alloc_bytes",
            "legendFormat": "Allocated",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
